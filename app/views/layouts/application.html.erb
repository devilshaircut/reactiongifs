<!DOCTYPE html>
<html>
<head>
  <title>Reactiongifs</title>
  <%= favicon_link_tag "/assets/favicon.ico" %>
  <%= stylesheet_link_tag    "bootstrap/bootstrap", "bootstrap/bootstrap-responsive", "reactiongifs" %>
  <%= javascript_include_tag "jquery", "bootstrap/bootstrap" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:notice] %>
<p class="flash-notice"><%= flash[:notice] %></p>
<% end %>
<% if flash[:alert] %>
<p class="flash-alert"><%= flash[:alert] %></p>
<% end %>

<%= yield %>

</body>
</html>
